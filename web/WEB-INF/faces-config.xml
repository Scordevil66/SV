<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>Empresa</from-outcome>
            <to-view-id>/Vistas/Empresa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Usuarios Masiva</from-outcome>
            <to-view-id>/Vistas/Usuarios_Masiva.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Comite</from-outcome>
            <to-view-id>/Vistas/Comite.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Usuario</from-outcome>
            <to-view-id>/Vistas/Usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Inventario</from-outcome>
            <to-view-id>/Vistas/Inventario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>Inventario Masiva</from-outcome>
            <to-view-id>/Vistas/Inventario_Masiva.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>Verificacion</from-outcome>
            <to-view-id>/Vistas/Verificacion.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Seleccion de Juguetes</from-outcome>
            <to-view-id>/Vistas/SeleccionJuguete.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>Votacion de Juguetes</from-outcome>
            <to-view-id>/Vistas/VotarJuguete.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>Seleccion</from-outcome>
            <to-view-id>/Vistas/Seleccion.xhtml</to-view-id>
            <redirect/>
        </navigation-case>   
        <navigation-case>
            <from-outcome>Votar</from-outcome>
            <to-view-id>/Vistas/Votar.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>Confirmacion Seleccion</from-outcome>
            <to-view-id>/Vistas/ConfirmacionSeleccion.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
        <navigation-case>
            <from-outcome>Dashboard</from-outcome>
            <to-view-id>/Vistas/Dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case> 
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>         
    </navigation-rule>  
    <managed-bean>  
        <managed-bean-name>empresaCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.EmpresaCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>  
        <managed-bean-name>ciudadCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.CiudadCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>  
        <managed-bean-name>departamentoCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.DepartamentoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jugueteCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.JugueteCT</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>usuarioCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.UsuarioCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pedidoCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.PedidoCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sesionCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.SesionCT</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>comiteCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.ComiteCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>votacionCT</managed-bean-name>
        <managed-bean-class>com.sv.controladores.VotacionCT</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
